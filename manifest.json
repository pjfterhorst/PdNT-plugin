{
    "manifest_version": 2,
    "name": "PdNt-response",
    "description": "This extension reads localStorage data",
    "version": "1.0",
    "permissions": ["storage", "<all_urls>"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": ["popup.js"]
  }
  